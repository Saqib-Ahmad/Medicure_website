<script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.min.js"></script>

<div id="user" class="container profile " >
  <div class="row">
    <div class="col text-center mt-3">
      <h2 class="mt-3" style="text-transform: capitalize;">{{ firstname }}</h2>
    </div>
  </div>

  <div class="row mt-2">
    <div class="col">
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item">
          <a class="nav-link active" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="true">Profile</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Contact</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="address-tab" data-toggle="tab" href="#address" role="tab" aria-controls="address" aria-selected="false">Address</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="links-tab" data-toggle="tab" href="#links" role="tab" aria-controls="links" aria-selected="false">Links</a>
        </li>
      </ul>
      <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="profile" role="tabpanel" aria-labelledby="profile-tab">
          <table class="table table-hover table-sm table-properties">
            <tr v-show="user.preferred_username">
              <th>Firstname:</th>
              <td>{{firstname}}</td>
            </tr>
            <tr v-show="user.preferred_username">
              <th>Lastname:</th>
              <td>{{lastname}}</td>
            </tr>
            <tr v-show="user.password">
              <th>Password:</th>
              <td>{{password}}</td>
            </tr>
            <tr v-show="user.name_of_hospital">
              <th>Name of hospital:</th>
              <td>{{hospital}}</td>
            </tr>
            <tr v-show="user.locale">
              <th>Treatment of Disease:</th>
              <td>{{disease}}</td>
            </tr>
            <tr v-show="user.nickname">
              <th>Doctor Appointed:</th>
              <td>{{doctor}}</td>
            </tr>
            
            <tr v-show="user.gender">
              <th>Gender:</th>
              <td>{{gender}}</td>
            </tr>
            <tr v-show="user.birthdate">
              <th>Birthdate:</th>
              <td >{{dob}}</td>
            </tr>
            
            <tr v-show="user.zoneinfo">
              <th>Blood Group:</th>
              <td>{{blood}}</td>
            </tr>
            
          </table>
        </div>

        <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
          <table class="table table-hover table-sm table-properties">
            <tr v-show="user.email">
              <th>Email Id:</th>
              <td>{{email}}</td>
            </tr>
            <tr v-show="user.phone_number">
              <th>Phone Number:</th>
              <td>{{number}}</td>
            </tr>
          </table>
        </div>

        <div class="tab-pane fade" id="address" role="tabpanel" aria-labelledby="address-tab">
          <table class="table table-hover table-sm table-properties">
          <tr v-show="user.address.street_address">
              <th>Address:</th>
              <td>{{Address}}</td>
            </tr>
            <tr v-show="user.address.country">
              <th>City:</th>
              <td>{{city}}</td>
            </tr>
            <tr v-show="user.address.postal_code">
              <th>Pincode:</th>
              <td>{{pincode}}</td>
            </tr>
          </table>
        </div>

        <div class="tab-pane fade" id="links" role="tabpanel" aria-labelledby="links-tab">
          <table class="table table-hover table-sm table-properties">
          <tr v-show="user.website">
              <th>Hospital website:</th>
              <td><a v-bind:href="website">{{website}}</a></td>
            </tr>
            <tr v-show="user.me">
              <th>Medical report:</th>
              <td><a v-bind:href="medical">{{medical}}</a></td>
            </tr>
            
            <tr v-show="user.profile">
              <th>Bill Payment:</th>
              <td><a v-bind:href="bill">{{bill}}</a></td>
            </tr>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>


    <!-- ======= helpline Section ======= -->
